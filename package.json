        "@btc-vision/transaction": "^1.6.17",
        "@btc-vision/unit-test-framework": "^0.3.5",
        "gulp-logger-new": "^1.0.1",
        "npm-run-all": "^4.1.5",
        "opnet": "^1.6.31",
        "stream-chain": "^3.4.0",
        "stream-json": "^1.9.1"
    },
    "devDependencies": {
        "@eslint/js": "^9.33.0",
        "@types/node": "^24.2.1",
        "@types/stream-json": "^1.7.8",
        "@typescript-eslint/eslint-plugin": "^8.39.1",
        "@typescript-eslint/parser": "^8.39.1",
        "eslint": "^9.33.0",
        "gulp": "^5.0.1",
        "gulp-cached": "^1.1.1",
        "gulp-clean": "^0.4.0",
        "gulp-eslint-new": "^2.4.0",
        "gulp-typescript": "^6.0.0-alpha.1",
        "prettier": "^3.6.2",
        "tsx": "^4.20.3",
        "typescript": "^5.9.2",
        "typescript-eslint": "^8.39.1"
