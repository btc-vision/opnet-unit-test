        "test:native:getquote": "tsx src/tests/native-swap/GetQuote.ts",
        "test:native:getreserve": "tsx src/tests/native-swap/GetReserve.ts",
        "test:native:reserve": "tsx src/tests/native-swap/Reserve.ts",
        "test:native:pause": "tsx src/tests/native-swap/Pause.ts",
        "test:native:swap": "tsx src/tests/native-swap/Swap.ts",
        "test:native:withdraw": "tsx src/tests/native-swap/Withdraw.ts",
        "test:native": "npm-run-all test:native:cancellisting test:native:onlyowner test:native:reentrancy test:native:listtoken test:native:createpool test:native:purge test:native:addliquidity test:native:stakingfee test:native:fee test:native:fees test:native:staking test:native:getquote test:native:getreserve test:native:reserve test:native:pause test:native:swap test:native:withdraw",
        "test:track": "tsx src/tests/native-swap/TrackPriceBalance.ts",
